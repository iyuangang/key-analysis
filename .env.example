# PostgreSQL 配置
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=key_analysis
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis 配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# 后端配置
BACKEND_SECRET_KEY=your_secret_key_here
BACKEND_CORS_ORIGINS=["http://localhost:5173","https://localhost"]
BACKEND_PORT=8000

# 前端配置
VITE_API_BASE_URL=http://localhost:8000

# Nginx 配置
NGINX_HOST=localhost
NGINX_PORT=80
NGINX_SECURE_PORT=443

# 应用配置
APP_ENV=development
DEBUG=true
LOG_LEVEL=debug

# 安全配置
JWT_EXPIRATION=3600
RATE_LIMIT_PER_SECOND=10
MAX_REQUEST_SIZE=10mb

# 缓存配置
CACHE_TTL=300
SESSION_TTL=86400
CONFIG_TTL=3600
